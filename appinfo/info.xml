<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>cuckoo</id>
    <name>cuckoo</name>
    <summary>Send file directly to cuckoo sandbox</summary>
    <description>
        Allows users to send file to cuckoo sandbox without download it.

	This plug-in is an "quick and dirty" implemetation based on "checksum" plugin
	developed by westberliner.
    </description>
    <version>0.1.0</version>
    <licence>agpl</licence>
    <author>Luca Memini</author>
    <types>
      <filesystem/>
    </types>
    <documentation>
        <admin>https://github.com/LDO-CERT/nextcloud-cuckoo/blob/master/README.md</admin>
    </documentation>
    <category>files</category>
    <category>security</category>
    <website>https://github.com/LDO-CERT/nextcloud-cuckoo/</website>
    <bugs>https://github.com/LDO-CERT/nextcloud-cuckoo/issues</bugs>
    <dependencies>
        <owncloud min-version="9.0" max-version="11" />
        <nextcloud min-version="9" max-version="16" />
    </dependencies>
</info>
